#include<stdio.h>
int main()
{
    int t,i;
    scanf("%d",&t);
    for(i=1;i<=t;i++)
    {
        int m,n;
        scanf("%d%d",&m,&n);
        if(m>n)
        {
            printf(">\n");
        }
        else if(m<n)
        {
            printf("<\n");
        }
        else if(m=n)
        {
            printf("=\n");
        }
    }
    return 0;
}
