#include<stdio.h>
int main()
{
    long long int m,n,difference;
    while(scanf("%lld%lld", &m,&n) ==2)
        {
            if(m>n)
                {
                    difference=m-n;
                    printf("%lld\n",difference);
                }
            else
                {
                    difference=n-m;
                    printf("%lld\n",difference);
                }
        }
    return 0;
}
