
#include<stdio.h>
int main()
{
    int T,i,cs=0;
    scanf("%d",&T);
    while(T--)
    {
        int a,b;
        scanf("%d%d",&a,&b);
        int sum=0;
        for(i=a;i<=b;i++)
        {
            if(i%2!=0)
            {
                sum=sum+i;
            }
        }
        printf("Case %d: %d\n",++cs,sum);
    }
    return 0;
}
